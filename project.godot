; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Logi-5"
config/description="Template from https://github.com/DeybisMelendez/godot-template"
run/main_scene="res://scenes/main/Main.tscn"
config/icon="res://icon.png"

[autoload]

Globals="*res://autoloads/globals.gd"
Background="*res://autoloads/background/Background.tscn"
Audio="*res://autoloads/audio/Audio.tscn"

[display]

window/size/width=720
window/size/height=1280
window/size/test_width=360
window/size/test_height=640
window/stretch/mode="2d"
window/stretch/aspect="expand"

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.231373, 0.294118, 0.364706, 1 )
environment/default_environment="res://default_env.tres"
